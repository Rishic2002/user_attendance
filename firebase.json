{
  "functions": [
    {
      "source": "functions",
      "codebase": "default"
    },
    {
      "source": "h",
      "codebase": "h",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "auth": {
      "port": 9099
    },
    "hosting": {
      "port": 5000
    }
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
